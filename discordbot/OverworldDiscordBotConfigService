class OverworldDiscordBotConfigService():
    def __init__(self, config_file_name):
        self.config_file_name = config_file_name
        self.config = {}

    # Purge config in memory and load from file.
    def load_config(self):
        self.config = {}
        config_data = json.load(open("../" + self.config_file_name))
        self.config = config_data

